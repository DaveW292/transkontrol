### PROTECT FILES ###
<FilesMatch "\.(htaccess|htpasswd|ini|fla|psd|log|sh)$">
Order Allow,Deny
Deny from all
</FilesMatch>

RewriteEngine on
RewriteRule o-nas index.php
RewriteRule uslugi uslugi.php
RewriteRule platnosci platnosci.php
RewriteRule skargi skargi.php
RewriteRule odwolania odwolania.php
RewriteRule pozwy pozwy.php
RewriteRule media media.php
RewriteRule incydenty incydenty.php
RewriteRule praca praca.php
RewriteRule kontakt kontakt.php
RewriteRule polityka-prywatnosci polityka-prywatnosci.php
RewriteRule polityka-cookies polityka-cookies.php
